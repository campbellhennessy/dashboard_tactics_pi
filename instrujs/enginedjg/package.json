{
    "name": "enginedjg",
    "version": "1.98.103",
    "description": "HTML/CSS/JS based WebView dial instruments for OpenCPN Dashboard-Tactics plug-in",
    "main": "index.js",
    "scripts": {
        "enginedjg": "webpack -p --progress",
        "common": "babel-minify ../../data/instrujs/common.js --out-file ../../data/instrujs/common.min.js --mangle.keepClassName",
        "build": "npm run enginedjg && npm run common",
        "debug": "webpack -d --display-modules",
        "watch": "webpack -d --display-modules --watch",
        "serve": "cd ../../data/instrujs/; http-server",
        "test": "weinre"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/canne/dashboard_tactics_pi.git"
    },
    "keywords": [
        "chart",
        "plotter",
        "instruments",
        "NMEA-2000",
        "SignalK",
        "engine",
        "dial"
    ],
    "author": "Petri Makijarvi mailto:petri38-github@yahoo.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/canne/dashboard_tactics_pi/issues"
    },
    "homepage": "https://github.com/canne/dashboard_tactics_pi#readme",
    "dependencies": {
        "@babel/runtime": "^7.7.7",
        "javascript-state-machine": "3.1.0",
        "justgage": "1.3.5",
        "raphael": "2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.7.7",
        "autoprefixer": "^9.7.3",
        "babel-loader": "^8.0.6",
        "babel-minify": "^0.5.1",
        "css-loader": "^3.4.0",
        "cssnano": "^4.1.10",
        "exports-loader": "^0.7.0",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.12.0",
        "mini-css-extract-plugin": "^0.9.0",
        "sass": "^1.24.0",
        "sass-loader": "^8.0.0",
        "terser-webpack-plugin": "^2.3.1",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "weinre": "^2.0.0-pre-I0Z7U9OV"
    }
}
